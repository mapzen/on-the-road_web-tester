doctype html
html
  head
    link(href='static/app.css', rel='stylesheet')
    script(type='text/javascript', src='https://maps.googleapis.com/maps/api/js?key=AIzaSyAmNr8MoeIoHsuX-7npxkqZMBGto9sBM7s&sensor=false')
    script(src='http://code.jquery.com/jquery-1.10.1.min.js')
    script(src='https://api.tiles.mapbox.com/mapbox.js/v1.6.1/mapbox.js')
    link(href='https://api.tiles.mapbox.com/mapbox.js/v1.6.1/mapbox.css', rel='stylesheet')
    script(type='text/javascript', src='data?route_id=#{route_id}')
    script(type='text/javascript', src='static/app.js')
  body
    #other_routes
      h2 Routes
      ul
        for route in routes
          li
            a(href='?route_id=#{route._id}') #{route._id}
    #map(data-current-route="#{route_id}")
